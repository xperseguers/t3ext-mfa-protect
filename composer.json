{
  "name": "causal/mfa-protect",
  "type": "typo3-cms-extension",
  "description": "Protect rendering of sensitive content elements with MFA.",
  "keywords": [
    "TYPO3 CMS",
    "MFA",
    "secure",
    "security",
    "protection"
  ],
  "homepage": "https://github.com/xperseguers/t3ext-mfa-protect",
  "support": {
    "issues": "https://github.com/xperseguers/t3ext-mfa-protect/issues"
  },
  "authors": [
    {
      "name": "Xavier Perseguers",
      "email": "xavier@causal.ch",
      "homepage": "https://www.causal.ch",
      "role": "Developer"
    }
  ],
  "license": "GPL-3.0-or-later",
  "require": {
    "php": ">=7.4.0 <=7.4.99 || >=8.0.0 <=8.2.99",
    "typo3/cms-core": "^11 || ^12"
  },
  "autoload": {
    "psr-4": {
      "Causal\\MfaProtect\\": "Classes/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "0.1.x-dev"
    },
    "typo3/cms": {
      "extension-key": "mfa_protect"
    }
  }
}
